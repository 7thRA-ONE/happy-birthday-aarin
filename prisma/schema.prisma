datasource db {
  provider = "mongodb"
  url      = env("DATABASE_URL")
}

generator client {
  provider = "prisma-client-js"
}

model Wish {
  id        String   @id @default(auto()) @map("_id") @db.ObjectId
  wish      String
  from      String
  image     String?
  createdAt DateTime @default(now())
}
